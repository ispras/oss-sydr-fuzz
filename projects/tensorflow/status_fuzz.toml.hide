exit-on-time = 3600

[sydr]
args = "-l debug"
target = "/sydr/status_fuzz @@"

[libfuzzer]
set_cover_merge=true
path = "/fuzzer/status_fuzz"
args = "-rss_limit_mb=8192 /corpus/status"

[cov]
target = "/coverage/status_fuzz @@"
